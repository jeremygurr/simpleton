#!/bin/bash

log() {
local log_file=${log_file:-}

if [[ ! "${log_file:-}" ]]; then
  local job_part=last-update-attempt
  if [[ "$p_job_id" ]]; then
    job_part=$p_job_id
  fi
  log_file=$job_path/$job_part/log/first.olog
fi

if [[ ! -f "$log_file" ]]; then
  log_error "Log file doesn't exist: $log_file. Maybe this cell hasn't been updated yet?"
  return 1
fi

omni_navigator
}