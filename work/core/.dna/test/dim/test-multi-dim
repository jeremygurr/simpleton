$ mkdir -p test-cube/.dna

$ cube edit test-cube/.dna/context content='
  batch_dims=( env project service )
  out_path=out
  execute_update() { 
    echo "env=$env project=$project service=$service" >$batch_out_path/value
  }
  '

$ cell update test-cube env=dev project=my-web service=s1

$ cd test-cube/.cyto/batch
$ cd dim/env/dev/dim/project/my-web/dim/service/s1/out
$ grep 'env=dev project=my-web service=s1' value >/dev/null 

