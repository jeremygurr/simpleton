$ mkdir -p test-cube/.dna

$ cube edit test-cube/.dna/context content='
  batch_dims=( service )
  out_path=out

  expand_member_service() {
    local -r value=$value
    case $value in
      mw|my-web)
        new_value=my-web
      ;;
      *)
        new_value=
      ;;
    esac
    return 0
  }

  execute_update() { 
    echo "service=$service" >$batch_out_path/value
  }
  '

$ cell update test-cube service=mw

$ cd test-cube/.cyto/batch
$ cd dim/service/my-web/out
$ grep 'service=my-web' value >/dev/null 

