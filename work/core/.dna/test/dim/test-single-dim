$ mkdir -p test-cube/.dna

$ cube edit test-cube/.dna/context content='
  batch_dims=( env )
  out_path=out
  execute_update() { 
    echo "env=$env" >$batch_out_path/value
  }
  '

$ cell update test-cube env=prod1

$ cd test-cube/.cyto/batch
$ cd dim/env/prod1/out
$ grep 'env=prod1' value >/dev/null 

