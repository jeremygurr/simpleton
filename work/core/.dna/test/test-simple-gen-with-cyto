$ mkdir -p simple-gen/.dna simple-gen/.cyto

$ cube edit simple-gen/.dna/context content='
  execute_update() { 
    seq 1 20 >$cube_path/numbers
  }
  '

$ cell update simple-gen 

$ cd simple-gen
$ [[ -d .cyto ]]   # cyto should already be here
$ [[ -f numbers ]]
$ cat numbers | grep '^5$' >/dev/null

